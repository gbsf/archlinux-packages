# arg 1:  the new package version
post_install() {
cat << EOF
IMPORTANT
==> In order to use the firmware you have to agree to Intel's license located in 
==> /lib/firmware/LICENSE.ipw2100-fw else please remove this package
EOF
}

# arg 1:  the new package version
# arg 2:  the old package version
post_upgrade() {
post_install $1 
}

op=$1
shift
$op $*
